{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.vue",
    "src/preload/**/*"
, "src/renderer/public/lib/area.js"  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
